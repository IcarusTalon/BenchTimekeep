@model IEnumerable<MvcBenchCalendar.Models.UserModel>
@{
    ViewBag.Title = "Users";
}
<h2>
    Users</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            User Name
        </th>
        <th>
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
    
        <tr>
            <td>
                @Html.Label(item.User.UserName)
                @Html.Label(item.User.ProviderUserKey.ToString())
            </td>
            <td>
                <a href="@Url.Action("ChangePassword", "Account", new { providerUserKey = item.User.ProviderUserKey.ToString() })" title="Edit" ><span class="ui-state-default ui-corner-all">
                    <span class="ui-icon ui-icon-pencil"></span></span></a>
            </td>
            <td>
                <a href="@Url.Action("Delete", new { })" title="Delete" ><span class="ui-state-default ui-corner-all">
                    <span class="ui-icon ui-icon-trash"></span></span></a>
            </td>
        </tr>
    }
</table>
